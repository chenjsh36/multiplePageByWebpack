{
  "name": "multiplePageByWebpack",
  "version": "1.0.0",
  "description": "Webpack 构建多页面应用",
  "main": "webpack.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node npm-scripts/clean-before-build.js && webpack --progress --colors --bail",
    "dev": "node npm-scripts/clean-before-build.js && webpack --progress --colors --config ./webpack.dev.config.js",
    "dll": "node npm-scripts/clean-before-build.js && webpack --progress --colors --config ./webpack.dll.config.js && node npm-scripts/cp-dll.js",
    "watch": "webpack --progress --colors --watch --config ./webpack.dev.config.js",
    "start": "webpack-dev-server --inline --progress --compress --devtool eval --content-base build/",
    "startdev": "node webpack.dev.server.js",
    "display": "node npm-scripts/clean-before-build.js && webpack --progress --display --display-modules --colors --config ./webpack.dev.config.js",
    "cp": "node npm-scripts/cp-dll.js",
    "clean": "node npm-scripts/clean-before-build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenjsh36/multiplePageByWebpack.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chenjsh36/multiplePageByWebpack/issues"
  },
  "homepage": "https://github.com/chenjsh36/multiplePageByWebpack#readme",
  "devDependencies": {
    "css-loader": "^0.25.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "jquery": "^3.1.1"
  }
}
